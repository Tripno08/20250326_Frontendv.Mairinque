# Ambiente
NODE_ENV=test

# Servidor
PORT=3000
HOST=0.0.0.0

# API
NEXT_PUBLIC_API_URL=https://api-test.innerview-escola.com
REACT_APP_API_URL=https://api-test.innerview-escola.com

# Analytics
NEXT_PUBLIC_ANALYTICS_ID=test-analytics-id

# Autenticação
NEXTAUTH_URL=https://test.innerview-escola.com
NEXTAUTH_SECRET=test-secret-key-for-nextauth

# Banco de Dados
DATABASE_URL=postgres://postgres:postgres@localhost:5432/innerview_test
DATABASE_SSL=false

# Cache
REDIS_URL=redis://localhost:6379
REDIS_TTL=3600

# Email
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=test@innerview-escola.com
SMTP_PASS=test-email-password

# Storage
STORAGE_TYPE=local
STORAGE_PATH=./uploads
AWS_ACCESS_KEY_ID=test-aws-access-key
AWS_SECRET_ACCESS_KEY=test-aws-secret-key
AWS_BUCKET=innerview-test-bucket
AWS_REGION=us-east-1

# Monitoring
SENTRY_DSN=https://test-sentry-dsn.ingest.sentry.io/test
SENTRY_ENVIRONMENT=test

# Security
JWT_SECRET=test-jwt-secret-key
COOKIE_SECRET=test-cookie-secret-key
CORS_ORIGIN=https://test.innerview-escola.com

# Features
ENABLE_NOTIFICATIONS=true
ENABLE_FILE_UPLOAD=true
ENABLE_ANALYTICS=true

# Auth
NEXT_PUBLIC_AUTH_DOMAIN=test.auth0.com
NEXT_PUBLIC_AUTH_CLIENT_ID=test-auth0-client-id
NEXT_PUBLIC_AUTH_AUDIENCE=https://api-test.innerview-escola.com
NEXT_PUBLIC_AUTH_SCOPE="openid profile email"

# LTI
REACT_APP_LTI_CLIENT_ID=test_lti_client_id
REACT_APP_LTI_CLIENT_SECRET=test_lti_client_secret
REACT_APP_LTI_LAUNCH_URL=https://test.innerview-escola.com/lti/launch
REACT_APP_LTI_PLATFORM_URL=https://test.innerview-escola.com

# Microsoft
REACT_APP_MICROSOFT_CLIENT_ID=test_microsoft_client_id
REACT_APP_MICROSOFT_CLIENT_SECRET=test_microsoft_client_secret
REACT_APP_MICROSOFT_TENANT_ID=test_microsoft_tenant_id
REACT_APP_MICROSOFT_GRAPH_API_VERSION=v1.0

# Google
REACT_APP_GOOGLE_CLIENT_ID=test_google_client_id
REACT_APP_GOOGLE_CLIENT_SECRET=test_google_client_secret

# Webhooks
REACT_APP_WEBHOOK_SECRET=test_webhook_secret
REACT_APP_WEBHOOK_TIMEOUT=5000
REACT_APP_WEBHOOK_RETRY_COUNT=3
